@foreach (var item in (IEnumerable<string>)ViewData["Conditions"])
{
    if (item.Contains("Heading"))
    {
        <div class="card" style="margin-top: 20px">
            <div class="card-body">
                <div class="row">
                    <h3>@item.Replace("Heading", "")   </h3>
                </div>
            </div>
        </div>
    }

    else
    {
        <div class="col-md-3 text-left">@item</div>
    }

}